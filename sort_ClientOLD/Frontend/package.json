{
  "name": "prismq-client-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "coverage": "vitest run --coverage --config vitest.config.ts",
    "test:e2e": "playwright test --config ../playwright.config.ts",
    "test:e2e:ui": "playwright test --ui --config ../playwright.config.ts",
    "test:e2e:report": "playwright show-report _meta/tests/e2e-results",
    "lint": "eslint src --ext .ts,.vue"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.6",
    "vitest": "^4.0.6",
    "vue-tsc": "^2.2.0"
  }
}
