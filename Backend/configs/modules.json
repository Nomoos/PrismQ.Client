    {
      "id": "youtube-shorts",
      "name": "YouTube Shorts Source",
      "description": "Collect YouTube Shorts content from trending, specific channels, or keyword searches",
      "category": "Sources/Content/Shorts",
      "version": "2.0.0",
      "script_path": "../../Sources/Content/Shorts/YouTube/src/cli.py",
      "parameters": [
        {
          "name": "mode",
          "type": "select",
          "options": [
            "trending",
            "channel",
            "keyword"
          ],
          "default": "trending",
          "label": "Scraping Mode",
          "description": "Choose how to collect YouTube Shorts",
          "required": true
        },
        {
          "name": "channel_url",
          "type": "text",
          "default": "",
          "label": "Channel URL",
          "description": "YouTube channel URL, handle (@username), or ID",
          "placeholder": "https://www.youtube.com/@channelname",
          "required": false,
          "conditional_display": {
            "field": "mode",
            "value": "channel"
          },
          "validation": {
            "pattern": "^(https?://)?(www\\.)?youtube\\.com/([@\\w-]+|channel/[\\w-]+)|@[\\w-]+|UC[\\w-]+$",
            "message": "Must be a valid YouTube channel URL, handle, or ID"
          }
        },
        {
          "name": "query",
          "type": "text",
          "default": "",
          "label": "Search Query",
          "description": "Keyword or phrase to search for",
          "placeholder": "startup ideas",
          "required": false,
          "conditional_display": {
            "field": "mode",
            "value": "keyword"
          },
          "warning": "⚠️ Keyword search is in development. Currently shows trending results. (Issue #300)"
        },
        {
          "name": "max_results",
          "type": "number",
          "default": 50,
          "min": 1,
          "max": 100,
          "label": "Maximum Results",
          "description": "Number of Shorts to collect",
          "required": false
        },
        {
          "name": "category",
          "type": "select",
          "options": [
            "All",
            "Gaming",
            "Music",
            "Entertainment",
            "Education"
          ],
          "default": "All",
          "label": "Category Filter",
          "description": "Filter trending Shorts by category",
          "required": false,
          "conditional_display": {
            "field": "mode",
            "value": "trending"
          }
        }
      ],
      "tags": [
        "content",
        "shorts",
        "youtube",
        "video",
        "channel",
        "trending"
      ],
      "status": "active",
      "enabled": true
